<h1>Listing Event Results</h1>

<table>
  <tr>
    <th>FileName</th>
    <th>Content</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @event_results.each do |result| %>
  <tr>
    <td><%= result.filename %></td>
    <td><%= result.content_type %></td>
    <td><%= link_to 'Show', result %></td>
    <td><%= link_to 'Destroy', result, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Event Result', '/event_results/create' %>
